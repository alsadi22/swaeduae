#!/usr/bin/env bash
set -euo pipefail
cd /var/www/swaeduae
bash tools/codex_pro.sh || true
tail -n 120 "$(ls -1t storage/logs/codex_pro_*.log | head -n1)" 2>/dev/null || true
echo "REPORT: storage/app/codex/REMEDIATION.md"
echo "LAST_STATUS: $(cat storage/app/codex/LAST_STATUS 2>/dev/null || echo 'n/a')"
